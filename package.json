{
  "pluginname": "去除链接重定向",
  "version": "1.9.5",
  "description": "去除网页内链接的重定向，具有高准确性和高稳定性，以及相比同类插件更低的时间占用，平均时间在0.02ms~0.05ms之间",
  "main": "./dist/去除链接重定向.user.js",
  "namespace": "Violentmonkey Scripts",
  "supportURL": "https://github.com/MerielVaren/remove-link-redirects/issues/new/choose",
  "homepage": "https://github.com/MerielVaren/remove-link-redirects",
  "scripts": {
    "test": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "watch": "cross-env NODE_ENV=development webpack --progress --watch",
    "changelog": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s -r 0",
    "deploy": "npm run build && npx gh-pages -d ./dist",
    "lint": "rome check --apply-unsafe --formatter-enabled true --organize-imports-enabled true --linter-enabled true --verbose ./src",
    "format": "rome format --write ./src"
  },
  "author": "Meriel",
  "license": "MIT",
  "keywords": [
    "greasy",
    "script",
    "javascript",
    "redirect"
  ],
  "devDependencies": {
    "@types/node": "18.16.10",
    "@types/webpack": "5.28.1",
    "cross-env": "7.0.3",
    "date-fns": "2.29.3",
    "rome": "^12.1.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "gm-http": "^0.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "p-retry": "^6.0.0"
  }
}
